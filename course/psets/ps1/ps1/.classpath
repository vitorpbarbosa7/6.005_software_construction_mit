<?xml version="1.0" encoding="UTF-8"?>
<classpath>
    <!-- Source paths -->
    <classpathentry kind="src" path="src"/>
    <classpathentry kind="src" path="test"/>
    
    <!-- JRE System Library for Java 17 -->
    <classpathentry kind="con" path="org.eclipse.jdt.launching.JRE_CONTAINER/org.eclipse.jdt.internal.debug.ui.launcher.StandardVMType/JavaSE-17"/>

    <!-- JUnit Library (version 4 in this case) -->
    <classpathentry kind="con" path="org.eclipse.jdt.junit.JUNIT_CONTAINER/4"/>

    <!-- External library reference (javax.json-1.0.jar) -->
    <classpathentry kind="lib" path="lib/javax.json-1.0.jar">
        <attributes>
            <attribute name="javadoc_location" value="http://docs.oracle.com/javaee/7/api/"/>
        </attributes>
    </classpathentry>

    <!-- Output path -->
    <classpathentry kind="output" path="bin"/>
</classpath>
